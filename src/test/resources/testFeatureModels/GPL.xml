		</rule>
		<rule>
			<eq>
				<conj>
					<var>WithNeighbors</var>
					<var>Undirected</var>
				</conj>
				<var>UndirectedWithNeighbors</var>
			</eq>
		</rule>
		<rule>
			<eq>
				<conj>
					<var>WithEdges</var>
					<var>Undirected</var>
				</conj>
				<var>UndirectedWithEdges</var>
			</eq>
		</rule>
	</constraints>
</featureModel>
