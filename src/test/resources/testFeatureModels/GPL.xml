					<conj>
						<var>MSTKruskal</var>
						<var>MSTPrim</var>
					</conj>
				</not>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>MSTKruskal</var>
				<var>WithEdges</var>
			</imp>
		</rule>
		<rule>
			<eq>
				<conj>
					<var>OnlyVertices</var>
					<var>Weighted</var>
				</conj>
				<var>WeightedOnlyVertices</var>
			</eq>
		</rule>
		<rule>
			<eq>
				<conj>
					<var>WithNeighbors</var>
					<var>Weighted</var>
